{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"TFTRoundScraper",
		"version":"1.0.0",
		"minimum-overwolf-version":"0.199.0.15",
		"author":"PrelaunchZeus2",
		"icon":"IconMouseOver.png",
		"icon_gray":"IconMouseNormal.png",
		"description":"This app collects your tft unit data on a round by round basis(I.e. unit positions, item slams, traits active, etc.)"
	},
    "permissions": ["Streaming","Hotkeys","GameInfo"],
	"data": {
        "game_targeting": {"type": "dedicated","game_ids": [54261]},
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
					"file":"index.html",
					"transparent": false,
					"resizable": true,
					"use_os_windowing": true,
					"size": {
						"width":700,
						"height":400
						},
					"min_size": {
						"width":400,
						"height":400
					}
				}
		}
	}
}